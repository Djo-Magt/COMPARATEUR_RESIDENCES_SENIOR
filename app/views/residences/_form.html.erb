<%= simple_form_for residence,
    data: {
      insert_in_list_target: "form",
      action: "submit->insert-in-list#send"
    } do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_night , input_html: { min: 1 } %>
  <%= f.input :number_of_guests , input_html: { min: 1 } %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
